{
  "name": "server",
  "version": "0.1.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --inspect --clear --transpile-only --no-notify -r tsconfig-paths/register src",
    "start": "node dist",
    "lint": "eslint src --ext=.ts --fix",
    "precommit": "lint-staged",
    "test": "jest --silent",
    "test:cov": "yarn run test --coverage",
    "test:e2e": "yarn run test -c jest-e2e.config.js --runInBand",
    "test:unit": "yarn run test -c jest-unit.config.js",
    "test:verbose": "yarn run test --silent=false",
    "typeorm": "ts-node-dev -r tsconfig-paths/register node_modules/.bin/typeorm"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/validator": "^13.1.3",
    "ts-node-dev": "^1.1.6"
  }
}
